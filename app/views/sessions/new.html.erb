<div class="log-in-form">
  <h1>Log In</h1>
  <br>
  <div class="form-signin">
    <%=form_tag log_in_path do%>
    <%=label_tag :email %>
    <%=text_field_tag :email %>
    <br><br>
    <%=label_tag :password %>
    <%=password_field_tag :password %>
    <br><br>
    <%=submit_tag "Log in", :class => "btn btn-primary" %>
    <%end%>
  </div>

</div>
