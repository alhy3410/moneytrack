<% if current_user %>
  You are currently logged in as <%= current_user.email %>
  <%= link_to "All Items", items_path, class: "btn black" %><br><br>
  <%= link_to "Log Out", log_out_path, class: "btn black" %>

<% else %>
  You are not currently logged in.
  <%= link_to "Log In", log_in_path, class: "btn black" %>
<% end %>
